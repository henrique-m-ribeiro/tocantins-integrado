{
  "name": "tocantins-integrado",
  "version": "1.0.0",
  "description": "Plataforma de SuperinteligÃªncia Territorial do Tocantins - MVP",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:dashboard\"",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:dashboard": "cd src/dashboard && npm run dev",
    "build": "npm run build:api && npm run build:dashboard",
    "build:api": "tsc -p tsconfig.api.json",
    "build:dashboard": "cd src/dashboard && npm run build",
    "start": "node dist/api/server.js",
    "migrate": "tsx src/database/migrate.ts",
    "seed": "tsx src/database/seed.ts",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts",
    "db:reset": "tsx src/database/reset.ts",
    "collect": "tsx src/collectors/index.ts",
    "collect:ibge": "tsx src/collectors/index.ts --source=ibge",
    "collect:inep": "tsx src/collectors/index.ts --source=inep",
    "collect:datasus": "tsx src/collectors/index.ts --source=datasus",
    "collect:siconfi": "tsx src/collectors/index.ts --source=siconfi",
    "collect:snis": "tsx src/collectors/index.ts --source=snis",
    "collect:persist": "tsx src/collectors/index.ts --persist",
    "test:pilot": "tsx src/tests/integration/test-palmas.ts",
    "validate": "tsx scripts/validate-deploy.ts",
    "data:collect": "tsx src/scripts/data-collection/index.ts",
    "data:process": "tsx src/scripts/data-processing/index.ts",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "openai": "^4.70.0",
    "pdfkit": "^0.15.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henrique-m-ribeiro/tocantins-integrado"
  },
  "author": "Henrique M. Ribeiro",
  "license": "MIT"
}
